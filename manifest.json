{
  "manifest_version": 3,
  "name": "Notion fragments",
  "version": "1.0",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "bg.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cs.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["page.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
